  "scripts": {
    "dev": "nodemon app",
    "start": "node app",
    "test": "mocha tests/*.js --exit"
  },
  "engines": {
    "node": "16.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jadiagaurang/ScreenshotCrew.git"
  },
  "keywords": [
    "screenshot",
    "screenshot-utility",
    "screenshot-generator",
    "screenshotscloud"
  ],
  "author": "Gaurang Jadia",
  "private": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jadiagaurang/ScreenshotCrew/issues"
  },
  "homepage": "https://github.com/jadiagaurang/ScreenshotCrew#readme",
  "devDependencies": {
    "mocha": "^10.0.0",
    "supertest": "^6.2.3"
  },
  "dependencies": {
    "aws-sdk": "^2.1128.0",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.7",
    "express": "^4.18.1",
    "memcached": "^2.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.16",
    "puppeteer": "^13.7.0",
